{
  "schemaVersion": 1,
  "id": "confetti",
  "version": "${version}",
  "name": "Confetti",
  "description": "Adds confetti particles to the game, which can be used by other mods",
  "authors": [],
  "contact": {
    "repo": "https://github.com/Tomate0613/confetti"
  },
  "license": "MIT",
  "icon": "assets/confetti/icon.png",
  "environment": "*",
  "entrypoints": {
    "client": [
      "dev.doublekekse.confetti.client.ConfettiClient"
    ],
    "main": [
      "dev.doublekekse.confetti.Confetti"
    ],
    "modmenu": [
      "dev.doublekekse.confetti.compat.modmenu.ModMenuApiImpl"
    ]
  },
  "mixins": [
    "confetti.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "1.21.x"
  },
  "accessWidener": "confetti.accesswidener"
}
